{
    "name": "arrounded/queues",
    "description": "A set of queue helpers",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^4.2",
        "illuminate/queue": "^4.2"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "phpunit/phpunit": "^4.7",
        "mockery/mockery": "^0.9.4",
        "phpspec/phpspec": "^2.2",
        "symfony/var-dumper": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Arrounded\\Queues\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arrounded\\Queues\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    }
}
